    <connection>sahayogi_test</connection>
    <sql>use PPIVSahayogiVBL;
Declare @MigDate date, @v_MigDate nvarchar(15), @BACID nvarchar(100)='080040101'; --&lt;000>MIGRA_&lt;AUD>_LAA

set @MigDate = (select Today from ControlTable);

select 'spool lrs_dmd_date_update.lst'
union all



/*
select a.flow_id from FINMIG..LRS a join FINMIG..ForAcidLAA b
on a.foracid = b.ForAcid join FINMIG..TotalLoan c on
b.MainCode = c.MainCode join Master m on m.MainCode = c.MainCode
where c.LoanType = 'NONEMI' and flow_id = 'PRDEM'
and m.LimitExpiryDate &lt;= @MigDate
*/



select 
'update tbaadm.lrs set flow_amt = ''0.01'' ,next_dmd_date = '''+replace(convert(varchar, m.LimitExpiryDate,106), ' ', '-')+''' where acid = (select acid from tbaadm.gam where foracid = '''+a.foracid+''') and FLOW_ID = ''EIDEM'';'
from FINMIG..LRS a join FINMIG..ForAcidLAA b
on a.foracid = b.ForAcid join FINMIG..TotalLoan c on
b.MainCode = c.MainCode join Master m on m.MainCode = c.MainCode
where c.LoanType = 'EMI' and flow_id = 'EIDEM'
and m.LimitExpiryDate &lt; @MigDate
union all 

select 'commit;' 
union all
select 'spool off;'
</sql>
